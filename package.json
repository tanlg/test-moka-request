{
  "name": "test-moka-request",
  "version": "1.0.7",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lingui:add-locale": "./node_modules/.bin/lingui add-locale",
    "lingui:pre-run": "node ./node_modules/mage-scripts/src/pre-run-lingui.js .",
    "lingui:post-run": "node ./node_modules/mage-scripts/src/post-run-lingui.js ./mage-i18n/",
    "lingui:compile": "npm run lingui:pre-compile && ./node_modules/.bin/lingui compile --strict --verbose && npm run lingui:post-run",
    "lingui:extract": "npm run lingui:pre-extract && rm -rf ./src/lingui/_build/src && ./node_modules/.bin/lingui extract --clean --verbose && npm run lingui:post-extract",
    "lingui:pre-extract": "npm run lingui:pre-run && node ./node_modules/mage-scripts/src/i18n-extract-compile-extension.js pre-extract mage-i18n/messages.csv src/lingui/en_US/messages.json src/lingui/zh_CN/messages.json",
    "lingui:post-extract": "node ./node_modules/mage-scripts/src/i18n-extract-compile-extension.js post-extract mage-i18n/messages.csv src/lingui/en_US/messages.json src/lingui/zh_CN/messages.json",
    "lingui:pre-compile": "node ./node_modules/mage-scripts/src/i18n-extract-compile-extension.js pre-compile mage-i18n/messages.csv src/lingui/en_US/messages.json src/lingui/zh_CN/messages.json",
    "lingui:result": "npx eslint -o lingui-result.html -f html src/main-app",
    "lingui:add-pseudo": "node ./node_modules/mage-scripts/src/i18n-pseudo-translation.js mage-i18n/messages.csv add",
    "lingui:remove-pseudo": "node ./node_modules/mage-scripts/src/i18n-pseudo-translation.js mage-i18n/messages.csv remove",
    "lint": "eslint src",
    "test": "jest",
    "test:w": "jest --watch",
    "prebuild": "npm run lint && npm test",
    "microbundle": "microbundle --sourcemap=false",
    "build": "NODE_ENV=production webpack --config webpack.config.js --mode production",
    "release": "sh ./scripts/release.sh"
  },
  "lingui": {
    "compileNamespace": "es",
    "fallbackLocale": "zh_CN",
    "format": "lingui",
    "localeDir": "<rootDir>/src/lingui",
    "sourceLocale": "zh_CN",
    "srcPathDirs": [
      "<rootDir>/src/"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.4",
    "@lingui/babel-plugin-extract-messages": "^2.9.1",
    "@lingui/cli": "^2.7.4",
    "@lingui/macro": "^2.7.4",
    "@lingui/react": "^2.7.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-macros": "^2.8.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-moka": "0.0.9",
    "global": "^4.4.0",
    "jest": "^26.1.0",
    "lodash": "^4.17.19",
    "mage-scripts": "git+ssh://git@gitlab.mokahr.com:ats-client/mage-scripts.git#1.0.6",
    "microbundle": "^0.12.3",
    "qs": "^6.9.4",
    "shelljs": "^0.8.4",
    "simple-git": "^2.13.2",
    "superagent": "^5.3.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
